<mvc:View xmlns="sap.m" 
	xmlns:mvc="sap.ui.core.mvc" controllerName="com.sagar.cbsapp.controller.App">
	<VBox>
		<Title text="Product List" titleStyle="H1"/>
		<Title text="Property binding" titleStyle="H2"/>
		<Text text="{path: 'product>/SayHello' }"/>
		<Text text="{product>/SayHello}"/>
		<Title text="Element binding" titleStyle="H2"/>
		<VBox binding="{product>/Supplier}">
			<Text text="Supplier name: {product>Name}"/>
			<Text text="Supplier address: {product>Address}"/>
		</VBox>
		<Title text="Aggregation binding" titleStyle="H2"/>
		<List items="{product>/Products}">
			<StandardListItem title="{product>Name}" description="{product>Price}"/>
		</List>
	</VBox>
</mvc:View>







